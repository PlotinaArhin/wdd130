        <mxCell id="HTTb59zFbpHm0OaErmfY-3" value="Title" style="text;strokeColor=#d6b656;fillColor=#fff2cc;html=1;fontSize=24;fontStyle=1;verticalAlign=middle;align=center;" vertex="1" parent="1">
